{
    "name": "inspiredminds/contao-lottie-player",
    "description": "Contao extension to integrate the Lottie Web Player for Lottie or Telegram Sticker JSON files.",
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/inspiredminds/contao-lottie-player",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
	"support": {
		"issues": "https://github.com/inspiredminds/contao-lottie-player/issues",
		"source": "https://github.com/inspiredminds/contao-lottie-player",
		"forum": "https://community.contao.org/de"
	},
	"funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "contao/core-bundle": "^4.9 || ^5.0",
        "symfony/config": "^4.4 || ^5.2 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.2 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.2 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.2 || ^6.0",
        "webmozart/path-util": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoLottiePlayer\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoLottiePlayer\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16 || ^3.0"
    }
}
